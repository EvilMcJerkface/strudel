 <environment extends="workers">
   <!--
   Environments to start/stop a system with MongoDB
   data stores. Specify: configServers, shardServers, mongos,
   and workerHosts, each of which is a string of space-separated host
   names
    -->
   <configServers/>
   <shardServers/>
   <mongos/>
   <param extends=".">
      <mongodb>
        <configsvr ref="configServers"/>
        <shardsvr ref="shardServers"/>
        <mongos ref="mongos"/>
      </mongodb>
   </param>
 </environment>
